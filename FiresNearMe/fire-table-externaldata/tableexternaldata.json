{
	"name": "Incident table external data",
	"description": "Table for Presenting Incident Data",
	"eula": "",
	"vendor": "SAP",
	"license": "",
	"id": "com.sap.sample.firetable.externaldata",
	"newInstancePrefix": "table",
	"version": "0.1.1",
	"icon": "data:image/png;base64,",
	"webcomponents": [
        {
			"kind": "main",
			"tag": "com-sap-sample-firetable-externaldata",
            "url": "https://fire-table-externaldata.s3-ap-southeast-2.amazonaws.com/tableexternaldata.js",
            "integrity": "",
            "ignoreIntegrity" : true
        }
    ],
	"properties": {
		"width": {
			"type": "integer",
			"default": 100
		},
		"height": {
			"type": "integer",
			"default": 100
		}
	},
	"methods": {
		"setData": {
            "description": "Sets the data for the table.",
            "parameters": [
				{
					"name": "newData",
					"type": "string",
					"description": "JSON Feed data"
				}
            ]
		},
		"setActiveRow": {
			"description": "Set active row",
            "parameters": [
				{
					"name": "row",
					"type": "string",
					"description": "Row Data"
				}
            ]
		},
		"getActiveRow": {
			"description": "Get active row",
            "returnType": "string"
		}
	},
	"events": {
		"onRowSelect": {
			"description": "rowSelected"
		}
	}
}
